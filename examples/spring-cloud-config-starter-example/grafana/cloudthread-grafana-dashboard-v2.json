{
  "__inputs": [
    {
      "name": "DS_PROMETHEUS",
      "label": "prometheus",
      "type": "datasource",
      "pluginId": "prometheus",
      "pluginName": "Prometheus"
    }
  ],
  "editable": true,
  "refresh": "5s",
  "schemaVersion": 41,
  "title": "Cloud Dynamic Thread Pool Dashboard",
  "uid": "uuid-321",
  "version": 23,
  "templating": {
    "list": [
      {
        "name": "application_name",
        "type": "query",
        "datasource": "${DS_PROMETHEUS}",
        "query": "label_values(application_name)",
        "current": {"text": "spring-cloud-config-starter-example","value": "spring-cloud-config-starter-example"}
      },
      {
        "name": "dynamic_thread_pool_id",
        "type": "query",
        "datasource": "${DS_PROMETHEUS}",
        "query": "label_values(dynamic_thread_pool_id)",
        "current": {"text": "cloudthread-producer","value": "cloudthread-producer"}
      }
    ]
  },
  "time": {"from": "now-1h","to": "now"},
  "panels": [
    {
      "type": "bargauge",
      "title": "Core Threads",
      "id": 1,
      "gridPos": {"h":7,"w":6,"x":0,"y":0},
      "datasource": "${DS_PROMETHEUS}",
      "targets":[{"expr":"dynamic_thread_pool_core_size{application_name=\"$application_name\",dynamic_thread_pool_id=\"$dynamic_thread_pool_id\"}","legendFormat":"{{instance}}","refId":"A"}],
      "fieldConfig": {
        "defaults": {
          "color":{"mode":"thresholds"},
          "thresholds":{"mode":"absolute","steps":[{"color":"green"}]}
        }
      }
    },
    {
      "type": "gauge",
      "title": "Maximum Threads",
      "id": 2,
      "gridPos": {"h":7,"w":6,"x":6,"y":0},
      "datasource": "${DS_PROMETHEUS}",
      "targets":[{"expr":"dynamic_thread_pool_maximum_size{application_name=\"$application_name\",dynamic_thread_pool_id=\"$dynamic_thread_pool_id\"}","legendFormat":"{{instance}}","refId":"A"}],
      "fieldConfig": {
        "defaults": {
          "color":{"mode":"thresholds"},
          "thresholds":{"mode":"absolute","steps":[{"color":"green"}]}
        }
      }
    },
    {
      "type": "timeseries",
      "title": "Current Threads",
      "id": 3,
      "gridPos": {"h":7,"w":12,"x":0,"y":7},
      "datasource": "${DS_PROMETHEUS}",
      "targets":[{"expr":"dynamic_thread_pool_current_size{application_name=\"$application_name\",dynamic_thread_pool_id=\"$dynamic_thread_pool_id\"}","legendFormat":"{{instance}}","refId":"A"}],
      "fieldConfig": {
        "defaults": {
          "color":{"mode":"thresholds"},
          "thresholds":{"mode":"absolute","steps":[{"color":"green"},{"value":0.75,"color":"orange"},{"value":0.9,"color":"red"}]}
        }
      }
    },
    {
      "type": "timeseries",
      "title": "Active Threads",
      "id": 4,
      "gridPos": {"h":7,"w":12,"x":0,"y":14},
      "datasource": "${DS_PROMETHEUS}",
      "targets":[{"expr":"dynamic_thread_pool_active_size{application_name=\"$application_name\",dynamic_thread_pool_id=\"$dynamic_thread_pool_id\"}","legendFormat":"{{instance}}","refId":"A"}],
      "fieldConfig": {
        "defaults": {
          "color":{"mode":"thresholds"},
          "thresholds":{"mode":"absolute","steps":[{"color":"green"},{"value":0.75,"color":"orange"},{"value":0.9,"color":"red"}]}
        }
      }
    },
    {
      "type": "timeseries",
      "title": "Queue Remaining Capacity",
      "id": 5,
      "gridPos": {"h":7,"w":8,"x":0,"y":21},
      "datasource": "${DS_PROMETHEUS}",
      "targets":[{"expr":"dynamic_thread_pool_queue_remaining_capacity{application_name=\"$application_name\",dynamic_thread_pool_id=\"$dynamic_thread_pool_id\"}","legendFormat":"{{instance}}","refId":"A"}],
      "fieldConfig": {
        "defaults": {
          "color":{"mode":"thresholds"},
          "thresholds":{"mode":"absolute","steps":[{"color":"green"},{"value":0.7,"color":"orange"},{"value":0.9,"color":"red"}]}
        }
      }
    },
    {
      "type": "timeseries",
      "title": "Queue Size",
      "id": 6,
      "gridPos": {"h":7,"w":8,"x":8,"y":21},
      "datasource": "${DS_PROMETHEUS}",
      "targets":[{"expr":"dynamic_thread_pool_queue_size{application_name=\"$application_name\",dynamic_thread_pool_id=\"$dynamic_thread_pool_id\"}","legendFormat":"{{instance}}","refId":"A"}],
      "fieldConfig": {
        "defaults": {
          "color":{"mode":"thresholds"},
          "thresholds":{"mode":"absolute","steps":[{"color":"green"},{"value":0.7,"color":"orange"},{"value":0.9,"color":"red"}]}
        }
      }
    },
    {
      "type": "timeseries",
      "title": "Rejected Tasks",
      "id": 7,
      "gridPos": {"h":7,"w":8,"x":16,"y":21},
      "datasource": "${DS_PROMETHEUS}",
      "targets":[{"expr":"dynamic_thread_pool_reject_count{application_name=\"$application_name\",dynamic_thread_pool_id=\"$dynamic_thread_pool_id\"}","legendFormat":"{{instance}}","refId":"A"}],
      "fieldConfig": {
        "defaults": {
          "color":{"mode":"thresholds"},
          "thresholds":{"mode":"absolute","steps":[{"color":"green"},{"value":1,"color":"orange"},{"value":5,"color":"red"}]}
        }
      }
    },
    {
      "type": "timeseries",
      "title": "Completed Tasks",
      "id": 8,
      "gridPos": {"h":7,"w":12,"x":0,"y":28},
      "datasource": "${DS_PROMETHEUS}",
      "targets":[{"expr":"dynamic_thread_pool_completed_task_count{application_name=\"$application_name\",dynamic_thread_pool_id=\"$dynamic_thread_pool_id\"}","legendFormat":"{{instance}}","refId":"A"}],
      "fieldConfig": {
        "defaults": {
          "color":{"mode":"thresholds"},
          "thresholds":{"mode":"absolute","steps":[{"color":"green"},{"value":0.75,"color":"orange"},{"value":0.9,"color":"red"}]}
        }
      }
    }
  ]
}